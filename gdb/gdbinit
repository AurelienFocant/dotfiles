set history save on
set history size unlimited
set history remove-duplicates 3
set history expansion on

set print pretty
set print object
set print symbol-filename on
set print array
set print union
set print vtbl

python
import os
glibc_src_path = "/usr/src/glibc/glibc-2.41"

if os.path.exists(glibc_src_path) :
	gdb.execute(f'set substitute-path ./ {glibc_src_path}')
end
